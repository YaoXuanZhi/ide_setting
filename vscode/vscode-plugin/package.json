{
    "name": "yxz-keybindings",
    "displayName": "yxz Keymap",
    "description": "本人自用的跨IDE快捷键",
    "version": "1.0.1",
    "publisher": "dever-yaoxuanzhi",
	"license": "MIT",
    "engines": {
        "vscode": "^0.10.1",
		"node": ">6.0.0"
    },
    "categories": [
        "Keymaps"
    ],
    "keywords": [
        "keymap",
        "yxz"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/YaoXuanZhi/ide_setting_sync/tree/main/vscode"
    },
    "bugs": {
        "url": "https://github.com/YaoXuanZhi/ide_setting_sync/issues"
    },
	"icon": "images/logo.png",

    "contributes": {
        "keybindings": [
            {
                "key": "alt+m",
                "command": "workbench.action.gotoSymbol"
            },
            {
                "key": "ctrl+shift+o",
                "command": "-workbench.action.gotoSymbol"
            },
            {
                "key": "alt+g",
                "command": "editor.action.revealDefinition",
                "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
            },
            {
                "key": "f12",
                "command": "-editor.action.revealDefinition",
                "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
            },
            {
                "key": "shift+alt+r",
                "command": "-remote-wsl.revealInExplorer",
                "when": "editorFocus && remoteName == 'wsl'"
            },
            {
                "key": "shift+alt+r",
                "command": "-revealFileInOS",
                "when": "!editorFocus"
            },
            {
                "key": "alt+f7",
                "command": "references-view.findReferences",
                "when": "editorHasReferenceProvider"
            },
            {
                "key": "shift+alt+f12",
                "command": "-references-view.findReferences",
                "when": "editorHasReferenceProvider"
            },
            {
                "key": "shift+alt+s",
                "command": "workbench.action.findInFiles"
            },
            {
                "key": "ctrl+shift+f",
                "command": "-workbench.action.findInFiles"
            },
            {
                "key": "shift+alt+o",
                "command": "workbench.action.quickOpen"
            },
            {
                "key": "ctrl+p",
                "command": "-workbench.action.quickOpen"
            },
            {
                "key": "shift+f12",
                "command": "-editor.action.goToReferences",
                "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
            },
            {
                "key": "ctrl+shift+space",
                "command": "editor.action.triggerParameterHints",
                "when": "editorHasSignatureHelpProvider && editorTextFocus"
            },
            {
                "key": "ctrl+shift+space",
                "command": "-editor.action.triggerParameterHints",
                "when": "editorHasSignatureHelpProvider && editorTextFocus"
            },
            {
                "key": "alt+i",
                "command": "editor.action.showDefinitionPreviewHover"
            },
            {
                "key": "ctrl+k ctrl+k",
                "command": "bookmarks.toggle",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+alt+k",
                "command": "-bookmarks.toggle",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+k shift+n",
                "command": "bookmarks.jumpToPrevious",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+alt+j",
                "command": "-bookmarks.jumpToPrevious",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+k ctrl+n",
                "command": "bookmarks.jumpToNext",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+alt+l",
                "command": "-bookmarks.jumpToNext",
                "when": "editorTextFocus"
            },
            {
                "key": "shift+f11",
                "command": "bookmarks.listFromAllFiles"
            },
            {
                "key": "shift+alt+r",
                "command": "editor.action.rename",
                "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
            },
            {
                "key": "f2",
                "command": "-editor.action.rename",
                "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
            },
            {
                "key": "shift+alt+r",
                "command": "debug.renameWatchExpression",
                "when": "watchExpressionsFocused"
            },
            {
                "key": "f2",
                "command": "-debug.renameWatchExpression",
                "when": "watchExpressionsFocused"
            },
            {
                "key": "shift+alt+l",
                "command": "-bookmarks.expandSelectionToNext",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+k",
                "command": "-extension.vim_ctrl+k",
                "when": "editorTextFocus && vim.active && vim.use<C-k> && !inDebugRepl"
            }
        ]
    },
	"__metadata": {
		"id": "9cc31dd7-7ec4-4a46-bd4f-7d7b59d322a",
		"publisherId": "dever-yaoxuanzhi",
		"publisherDisplayName": "耀轩之"
	}
}
